      "eject": "react-scripts eject",
      "prettier:write": "npx prettier --write **/*.{js,jsx,json} && npx prettier --write *.{js,jsx,json}"
    },
    "eslintConfig": {
      "extends": [
        "react-app",
        "react-app/jest"
      ]
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
