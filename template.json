{
  "package": {
    "dependencies": {
      "@emotion/react": "^11.7.0",
      "@emotion/styled": "^11.6.0",
      "@material-ui/core": "^4.12.3",
      "@mui/icons-material": "^5.2.0",
      "@mui/material": "^5.2.2",
      "formik": "^2.2.9",
      "global": "^4.4.0",
      "moment": "^2.29.1",
      "react-redux": "^7.2.6",
      "react-router-dom": "^5.3.0",
      "redux": "^4.1.2",
